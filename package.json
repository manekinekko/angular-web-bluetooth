  },
  "homepage": "https://github.com/manekinekko/angular-web-bluetooth",
  "bugs": "https://github.com/manekinekko/angular-web-bluetooth/issues",
  "description": "A Web Bluetooth (Bluetooth Low Energy) module for angular (v2+)",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "build:aot": "ngc -p tsconfig.json",
    "build:jit": "tsc -p tsconfig.json",
    "start": "npm run clean && npm run build:aot",
    "build": "npm run clean && npm run build:aot",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json -n 'Angular Web Bluetooth Documentation' --hideGenerator -s -o",
    "compodoc-prod": "./node_modules/.bin/compodoc -p tsconfig.json -d dist/documentation/ --hideGenerator"
  },
  "types": "dist/index.d.ts",
  "private": false,
  "keywords": [
    "angular",
    "angular2",
    "bluetooth",
    "low",
    "energy",
    "web",
    "ble",
    "iot",
    "sensor",
    "accelerometer",
    "luxometer",
    "transmitter",
    "magnetometer",
    "ir",
    "nfc",
    "thermometer",
    "puckjs",
    "sensortag"
  ],
  "dependencies": {
    "@angular/common": ">=4.0.0-beta <5.0.0",
    "@angular/compiler": ">=4.0.0-beta <5.0.0",
    "@angular/core": ">=4.0.0-beta <5.0.0",
    "@angular/platform-browser": ">=4.0.0-beta <5.0.0",
    "@types/web-bluetooth": "^0.0.2",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.7.6",
    "rimraf": "^2.5.4",
    "typescript": "~2.1.0"
  },
  "devDependencies": {
    "@angular/compiler-cli": ">=4.0.0-beta <5.0.0",
    "@angular/platform-server": ">=4.0.0-beta <5.0.0",
    "@compodoc/compodoc": "^1.0.3",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60"
  }
}
